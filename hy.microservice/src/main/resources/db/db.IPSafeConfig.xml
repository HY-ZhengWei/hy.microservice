<?xml version="1.0" encoding="UTF-8"?>

<config>

    <import name="sqls"            class="java.util.ArrayList" />
    <import name="sqlGroup"        class="org.hy.common.xml.plugins.XSQLGroup" />
    <import name="sql"             class="org.hy.common.xml.XSQL" />



    <!-- 系统安全访问IP黑白名单的相关SQL -->
    <sqls>
    
        <sql id="XSQL_Common_IPSafeConfig_Insert"> 
        
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content if="ORACLE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                INSERT  INTO TIPSafeConfig
                       (
                         id          
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,"COMMENT"
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                       )
                VALUES (
                         ':id'
                        ,':ipType'
                        ,':ip'
                        ,':ipMax'
                        ,':moduleCode'
                        ,':url'
                        ,':comment'
                        ,':isDel'
                        ,':createUserID'
                        ,SYSDATE
                        ,SYSDATE
                       )
                ]]>
            </content>
            
            <content if="MYSQL == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                INSERT  INTO TIPSafeConfig
                       (
                         id          
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,comment     
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                       )
                VALUES (
                         ':id'
                        ,':ipType'
                        ,':ip'
                        ,':ipMax'
                        ,':moduleCode'
                        ,':url'
                        ,':comment'
                        ,':isDel'
                        ,':createUserID'
                        ,NOW()
                        ,NOW()
                       )
                ]]>
            </content>
            
            <content if="SQLITE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                INSERT  INTO TIPSafeConfig
                       (
                         id          
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,comment     
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                       )
                VALUES (
                         ':id'
                        ,':ipType'
                        ,':ip'
                        ,':ipMax'
                        ,':moduleCode'
                        ,':url'
                        ,':comment'
                        ,':isDel'
                        ,':createUserID'
                        ,DATETIME('now')
                        ,DATETIME('now')
                       )
                ]]>
            </content>
            
        </sql>
        
        
        
        <sql id="XSQL_Common_IPSafeConfig_Update"> 
        
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content if="ORACLE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                UPDATE  TIPSafeConfig
                   SET  updateTime = SYSDATE
              <[       ,ipType     = ':ipType'      ]>
              <[       ,ip         = ':ip'          ]>
              <[       ,ipMax      = ':ipMax'       ]>
              <[       ,moduleCode = ':moduleCode'  ]>
              <[       ,url        = ':url'         ]>
              <[       ,"COMMENT"  = ':comment'     ]>
              <[       ,isDel      = ':isDel'       ]>
                 WHERE  id         = ':id'
                ]]>
            </content>
            
            <content if="MYSQL == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                UPDATE  TIPSafeConfig
                   SET  updateTime = NOW()
              <[       ,ipType     = ':ipType'      ]>
              <[       ,ip         = ':ip'          ]>
              <[       ,ipMax      = ':ipMax'       ]>
              <[       ,moduleCode = ':moduleCode'  ]>
              <[       ,url        = ':url'         ]>
              <[       ,comment    = ':comment'     ]>
              <[       ,isDel      = ':isDel'       ]>
                 WHERE  id         = ':id'
                ]]>
            </content>
            
            <content if="SQLITE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                UPDATE  TIPSafeConfig
                   SET  updateTime = DATETIME('now')
              <[       ,ipType     = ':ipType'      ]>
              <[       ,ip         = ':ip'          ]>
              <[       ,ipMax      = ':ipMax'       ]>
              <[       ,moduleCode = ':moduleCode'  ]>
              <[       ,url        = ':url'         ]>
              <[       ,comment    = ':comment'     ]>
              <[       ,isDel      = ':isDel'       ]>
                 WHERE  id         = ':id'
                ]]>
            </content>
            
        </sql>
        
        
        
        <sql id="XSQL_Common_IPSafeConfig_Query_ByIPType"> 
            
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content>
                <![CDATA[
                SELECT   id
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,comment
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                  FROM  TIPSafeConfig
                 WHERE  isDel  = 0
                   ANd  ipType = ':ipType'
                 ORDER  BY ipType
                          ,ip
                ]]>
            </content>
            
            <content if="ORACLE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                SELECT   id
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,"COMMENT"
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                  FROM  TIPSafeConfig
                 WHERE  isDel  = 0
                   ANd  ipType = ':ipType'
                 ORDER  BY ipType
                          ,ip
                ]]>
            </content>
            
            <result>
                <row>org.hy.microservice.common.ipSafe.IPSafeConfig</row>
                <cfill>setter(colValue)</cfill>
            </result>
            
        </sql>
        
        
        
        <sql id="XSQL_Common_IPSafeConfig_Query_ByIPType_ByPage"> 
            
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content ref="XSQL_Common_IPSafeConfig_Query_ByIPType.getContent" />
            <result  ref="XSQL_Common_IPSafeConfig_Query_ByIPType.getResult"/>
            <comment>分页查询</comment>
            
        </sql>
        
        
        
        <sql id="XSQL_Common_IPSafeConfig_Query_ByIPType_Count"> 
            
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content>
                <![CDATA[
                SELECT  COUNT(1) AS totalCount
                  FROM  TIPSafeConfig
                 WHERE  isDel  = 0
                   ANd  ipType = ':ipType'
                ]]>
            </content>
            
        </sql>
        
        
        
        <sql id="XSQL_Common_IPSafeConfig_Query_ByID"> 
            
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content>
                <![CDATA[
                SELECT   id
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url       
                        ,comment
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                  FROM  TIPSafeConfig
                 WHERE  id  = ':id'
                ]]>
            </content>
            
            <content if="ORACLE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                SELECT   id
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url       
                        ,"COMMENT"     
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                  FROM  TIPSafeConfig
                 WHERE  id  = ':id'
                ]]>
            </content>
            
            <result>
                <row>org.hy.microservice.common.ipSafe.IPSafeConfig</row>
                <cfill>setter(colValue)</cfill>
            </result>
            
        </sql>
        
        
        
        <sql id="XSQL_Common_IPSafeConfig_Query_All"> 
            
            <dataSourceGroup ref="DSG_MS_Common" />
            
            <content>
                <![CDATA[
                SELECT   id
                        ,ipType      
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,comment
                        ,isDel       
                        ,createUserID
                        ,createTime  
                        ,updateTime  
                  FROM  TIPSafeConfig
                 WHERE  isDel = 0
                 ORDER  BY ipType
                          ,ip
                ]]>
            </content>
            
            <content if="ORACLE == this.getDataSourceGroup.getDbProductType">
                <![CDATA[
                SELECT   id
                        ,ipType
                        ,ip
                        ,ipMax
                        ,moduleCode
                        ,url
                        ,"COMMENT"
                        ,isDel
                        ,createUserID
                        ,createTime
                        ,updateTime
                  FROM  TIPSafeConfig
                 WHERE  isDel = 0
                 ORDER  BY ipType
                          ,ip
                ]]>
            </content>
            
            <result>
                <table>org.hy.common.TablePartitionRID</table>
                <fill>putRow(row.ipType ,row.ipSafeKey ,row)</fill>
                <row>org.hy.microservice.common.ipSafe.IPSafeConfig</row>
                <cfill>setter(colValue)</cfill>
            </result>
            
        </sql>
        
    </sqls>
    
</config>